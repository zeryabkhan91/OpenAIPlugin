{
  "name": "OpenAIPlugin",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "webpack serve --mode development",
    "validate": "python3 scripts/validate.py",
    "prepare-release": "npm run build && npm run validate"
  },
  "keywords": ["openai", "plugin", "ai", "api", "status"],
  "author": "YourName",
  "license": "MIT",
  "description": "OpenAI API status and key validity monitoring for BrainDrive services.",
  "dependencies": {
    "html-webpack-plugin": "^5.6.3",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.3",
    "@mui/material": "^5.14.4",
    "@tailwindcss/postcss": "^4.0.12",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^9.2.1",
    "css-loader": "^7.1.2",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.0.12",
    "ts-loader": "^9.5.2",
    "typescript": "^5.7.3"
  },
  "braindrive": {
    "pluginType": "frontend",
    "initializer": "plugin_initializer.py",
    "compatibility": ">=1.0.0",
    "category": "ai",
    "permissions": ["network.read", "storage.read", "storage.write"]
  }
}
